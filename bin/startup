#!/bin/sh
set -ue
mod=alt

key()
{
	# see /usr/include/X11/keysymdef.h, /usr/include/X11/XF86keysym.h
	xdotool key --clearmodifiers "$@"
}

type()
{
	xdotool type --clearmodifiers "$@"
}

~/.xinitrc

key $mod+1
env-mapdone

key $mod+2
env-mapdone-playground

keynav & # ctrl+; hjkl, shift+hjkl, space
